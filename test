from oauth2client.service_account import ServiceAccountCredentials
import gspread

# Define your URL, scope, and credentials
url = "https://docs.google.com/spreadsheets/d/1WJEOfHH5TXH1-EuWHqPptbt4tVG43VXqwBnzkqbujbQ/edit#gid=0"
scope = ['https://www.googleapis.com/auth/spreadsheets']
credentials = ServiceAccountCredentials.from_json_keyfile_name('keys.json', scope)

# Authorize and open the worksheet
client = gspread.authorize(credentials)
sheet = client.open_by_url(url)
worksheet = sheet.get_worksheet(0)

# Define the starting row and column index (E is the 5th column)
start_row = 3
start_column = 5  # E is the 5th column

# Fetch data from the specified range
data = worksheet.get(start_row, start_column, worksheet.row_count, 1)

# Print the fetched data
for row in data:
    print(row[0])
